      <TextField fx:id="companyAddrField" layoutX="71.0" layoutY="94.0" prefHeight="25.0" prefWidth="355.0" promptText="Address" />
      <TextField fx:id="companyEmailField" layoutX="71.0" layoutY="163.0" promptText="E-mail" />
      <TextField fx:id="companyTaxNOField" layoutX="275.0" layoutY="163.0" promptText="Tax NO" />
      <TextField fx:id="companyFaxField" layoutX="71.0" layoutY="211.0" promptText="Fax" />
